-
-        # call texmacs for editing
-        self.call_texmacs(scheme_cmd, tm_equation, tm_style)
-
-        svg_name = self.tmp_name + ".svg" #if successful texmacs creates that svg file
-        if os.path.isfile(svg_name):
-           f = open(svg_name, 'r')
-           tree = etree.parse(f)
-           f.close()
-
-           root = tree.getroot()
-           new_node = root.find('{%s}g' % SVG_NS)
-
-           # -- Replace
